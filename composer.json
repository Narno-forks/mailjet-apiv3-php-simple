{
    "name": "mailjet/mailjet-apiv3-php-simple",
    "type": "library",
    "description": "[API v3] Simple PHP wrapper for the Mailjet API",
    "keywords": ["mailjet", "api", "v3", "php"],
    "homepage": "https://github.com/mailjet/mailjet-apiv3-php-simple/",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Collas",
            "email": "ccollas@mailjet.com",
            "homepage": "http://charlescollas.fr/",
            "role": "Developer Evangelist"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-4" : {
            "Mailjet\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mailjet\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
